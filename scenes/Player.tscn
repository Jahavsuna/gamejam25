[sub_resource type="GDScript" id="GDScript_70d11"]
script/source = "extends CollisionShape2D

func _on_area_2d_area_entered(area: Area2D) -> void:
	var loop_line = area.get_parent()
	var safe_zone = loop_line.safe_zone
	print(\"safe zone = \", safe_zone, \"area zone = \", area.zone_i)
	if area.zone_i == safe_zone: return
	
	var dest_segment: int = loop_line.loop_to_segment
	print(\"loop zone to segment #\", dest_segment)
	
	GameGlobals.perform_loop(dest_segment)
"

